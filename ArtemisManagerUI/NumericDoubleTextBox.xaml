<UserControl x:Class="ArtemisManagerUI.NumericDoubleTextBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ArtemisManagerUI"
             mc:Ignorable="d" Name="uc">
    <TextBox HorizontalAlignment="Stretch" Text="{Binding ElementName=uc, Path=Value, Mode=TwoWay}" PreviewTextInput="OnPreviewPortInput" />
</UserControl>
