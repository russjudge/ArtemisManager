<UserControl x:Class="ArtemisManagerUI.ArtemisINIControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ArtemisManagerUI"
             mc:Ignorable="d" Name="uc"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <GroupBox Header="Localized Settings">
            <Grid
                local:GridHelpers.ColumnCount="2"
                local:GridHelpers.RowCount="{Binding ElementName=uc, Path=SettingsFile.Settings.Keys.Count, Mode=OneWay}">
                <Label
                    Grid.Row="0" Grid.Column="0"
                    >
                    Network Port:
                </Label>
                

            </Grid>
        </GroupBox>
    </StackPanel>
</UserControl>
