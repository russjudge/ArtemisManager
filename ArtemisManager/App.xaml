<Application x:Class="ArtemisManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ArtemisManager"
             xmlns:ui="clr-namespace:ArtemisManagerUI;assembly=ArtemisManagerUI"
             StartupUri="pack://application:,,,/ArtemisManagerUI;component/Main.xaml"
             Startup="OnStartup" DispatcherUnhandledException="OnError" >
    <Application.Resources>
        <Style TargetType="Button">
            <Setter Property="Padding" Value="5,5,5,5" />
            <Setter Property="Margin" Value="2,2,2,2" />
        </Style>
    </Application.Resources>
    
</Application>
